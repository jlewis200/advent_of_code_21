inp w
mul x 0
add x z
mod x 26
div z (1|26)
add x -*\d+
eql x w
eql x 0
mul y 0
add y 25
mul y x
add y 1
mul z y
mul y 0
add y w
add y -*\d+
mul y x
add z y


inp w\nmul x 0\nadd x z\nmod x 26\ndiv z (1|26)\nadd x -*\d+\neql x w\neql x 0\nmul y 0\nadd y 25\nmul y x\nadd y 1\nmul z y\nmul y 0\nadd y w\nadd y -*\d+\nmul y x\nadd z y\n


inp w
mul x 0
add x z
mod x 26        x = z % 26
div z (1|26)    z = z if const == 1 else 0
add x -*\d+     x += const
eql x w
eql x 0         x = not (x == w)
mul y 0
add y 25
mul y x
add y 1         y = 26 if x else 1
mul z y	    	z *= y
mul y 0
add y w
add y -*\d+
mul y x
add z y         z += (w + const) * x