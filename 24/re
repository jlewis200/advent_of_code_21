inp w
mul x 0
add x z
mod x 26	x = z % 26
div z 26    z /= 26
add x -11	x += -11
eql x w
eql x 0		x = not (x == w)
mul y 0
add y 25
mul y x
add y 1		y = 26 if x else 1
mul z y		z = 0 (z *= y)
mul y 0
add y w
add y 3
mul y x
add z y		z = (w + 3) * x

w = z % 26

